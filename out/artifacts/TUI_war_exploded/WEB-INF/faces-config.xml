<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>database</from-outcome>
            <to-view-id>/faces/database.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>user</from-outcome>
            <to-view-id>/faces/user.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>requests</from-outcome>
            <to-view-id>/faces/requests.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/faces/main.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>stock</from-outcome>
            <to-view-id>/faces/stock.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>stock_requests</from-outcome>
            <to-view-id>/faces/stock_requests.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_request</from-outcome>
            <to-view-id>/faces/new_request.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_hresource</from-outcome>
            <to-view-id>/faces/new_hresource.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_nresource</from-outcome>
            <to-view-id>/faces/new_nresource.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>new_resource</from-outcome>
            <to-view-id>/faces/new_resource.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>register</from-outcome>
            <to-view-id>/faces/public/register.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>